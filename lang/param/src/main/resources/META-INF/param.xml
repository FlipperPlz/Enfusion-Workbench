<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <fileType
                implementationClass="com.flipperplz.bisutils.languages.param.ParamFileType"
                fieldName="instance"

                name="Param"
                language="Param"
                fileNamesCaseInsensitive="config.cpp"
                extensions="rvmat;cpp"
        />

        <lang.parserDefinition
                language="Param"
                implementationClass="com.flipperplz.bisutils.languages.param.parser.ParamParserDefinition"
        />
    </extensions>
</idea-plugin>